#include <stdio.h>
void main()
{
  int n1,n2,ch;
  printf("Enter two numbers:\n");
  scanf("%d%d",&n1,&n2);
  printf("1-Add\n2-sub\n3-multi\n4-div\n");
  printf("Enter your choice:");
  scanf("%d",&ch);
  if(ch==1)
  {
    printf("Add=%d",n1+n2);
  }
  else if(ch==2)
    printf("Sub=%d",n1-n2);
  else if(ch==3)
      {
        printf("Multi=%d",n1*n2);
      }
  else if(ch==4)
    if(n2==0)
      printf("Division by zero error");
    else
        printf("Div=%d",n1/n2);
  else
        printf("invaild choice");
}  

